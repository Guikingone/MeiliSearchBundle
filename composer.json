{
  "name": "guikingone/meili-search-bundle",
  "description": "A Symfony bundle that enable MeiliSearch",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": ["bundle", "symfony", "meili"],
  "authors": [
    {
      "name": "Guillaume Loulier",
      "email": "contact@guillaumeloulier.fr"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "meilisearch/meilisearch-php": "^0.10.2",
    "psr/log": "1.1.3",
    "symfony/config": "4.4.8",
    "symfony/console": "4.4.8",
    "symfony/dependency-injection": "4.4.8",
    "symfony/http-kernel": "4.4.8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "infection/infection": "0.15.3",
    "phpstan/phpstan": "^0.12.23",
    "phpunit/phpunit": "8.5.5",
    "symfony/event-dispatcher": "4.4.8",
    "symfony/http-client": "4.4.8"
  },
  "autoload": {
    "psr-4": {
      "MeiliSearchBundle\\": "",
      "MeiliSearchBundle\\Client\\": "Client/",
      "MeiliSearchBundle\\Command\\": "Command/",
      "MeiliSearchBundle\\DataCollector\\": "DataCollector/",
      "MeiliSearchBundle\\DependencyInjection\\": "DependencyInjection/",
      "MeiliSearchBundle\\Event\\": "Event/",
      "MeiliSearchBundle\\Exception\\": "Exception/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/",
      "Tests\\Client\\": "Tests/Client/",
      "Tests\\Command\\": "Tests/Command/",
      "Tests\\DataCollector\\": "Tests/DataCollector/",
      "Tests\\DependencyInjection\\": "Tests/DependencyInjection/",
      "Tests\\Event\\": "Tests/Event/"
    }
  }
}
