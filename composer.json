{
	"name": "guikingone/meili-search-bundle",
	"description": "A Symfony bundle that enable MeiliSearch",
	"minimum-stability": "stable",
	"license": "MIT",
	"keywords": ["bundle", "symfony", "meilisearch", "search"],
	"type": "symfony-bundle",
	"authors": [
		{
			"name": "Guillaume Loulier",
			"email": "contact@guillaumeloulier.fr"
		}
	],
	"autoload": {
		"psr-4": {
			"MeiliSearchBundle\\": "src/",
			"MeiliSearchBundle\\Bridge\\": "src/Bridge/",
			"MeiliSearchBundle\\Bridge\\Doctrine\\": "src/Bridge/Doctrine/",
			"MeiliSearchBundle\\Bridge\\Doctrine\\Attribute\\": "src/Bridge/Doctrine/Attribute/",
			"MeiliSearchBundle\\Bridge\\Doctrine\\Attribute\\Reader\\": "src/Bridge/Doctrine/Attribute/Reader/",
			"MeiliSearchBundle\\Bridge\\Doctrine\\EventSubscriber\\": "src/Bridge/Doctrine/EventSubscriber/",
			"MeiliSearchBundle\\Bridge\\Doctrine\\Serializer\\": "src/Bridge/Doctrine/Serializer/",
			"MeiliSearchBundle\\Cache\\": "src/Cache/",
			"MeiliSearchBundle\\Command\\": "src/Command/",
			"MeiliSearchBundle\\DataCollector\\": "src/DataCollector/",
			"MeiliSearchBundle\\DataProvider\\": "src/DataProvider/",
			"MeiliSearchBundle\\DependencyInjection\\": "src/DependencyInjection/",
			"MeiliSearchBundle\\Document\\": "src/Document/",
			"MeiliSearchBundle\\Dump\\": "src/Dump/",
			"MeiliSearchBundle\\Event\\": "src/Event/",
			"MeiliSearchBundle\\Event\\Document\\": "src/Event/Document/",
			"MeiliSearchBundle\\Event\\Dump\\": "src/Event/Dump/",
			"MeiliSearchBundle\\Event\\Index\\": "src/Event/Index/",
			"MeiliSearchBundle\\EventSubscriber\\": "src/EventSubscriber/",
			"MeiliSearchBundle\\Exception\\": "src/Exception/",
			"MeiliSearchBundle\\Form\\": "src/Form/",
			"MeiliSearchBundle\\Form\\Type\\": "src/Form/Type/",
			"MeiliSearchBundle\\Health\\": "src/Health/",
			"MeiliSearchBundle\\Index\\": "src/Index/",
			"MeiliSearchBundle\\Loader\\": "src/Loader/",
			"MeiliSearchBundle\\Messenger\\": "src/Messenger/",
			"MeiliSearchBundle\\Messenger\\Document\\": "src/Messenger/Document/",
			"MeiliSearchBundle\\Messenger\\Handler\\": "src/Messenger/Handler/",
			"MeiliSearchBundle\\Messenger\\Handler\\Document\\": "src/Messenger/Handler/Document/",
			"MeiliSearchBundle\\Messenger\\Handler\\Synonyms\\": "src/Messenger/Handler/Synonyms/",
			"MeiliSearchBundle\\Messenger\\Synonyms\\": "src/Messenger/Synonyms/",
			"MeiliSearchBundle\\Metadata\\": "src/Metadata/",
			"MeiliSearchBundle\\Result\\": "src/Result/",
			"MeiliSearchBundle\\Search\\": "src/Search/",
			"MeiliSearchBundle\\Twig\\": "src/Twig/",
			"MeiliSearchBundle\\Update\\": "src/Update/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\MeiliSearchBundle\\": "tests/",
			"Tests\\MeiliSearchBundle\\Bridge\\": "tests/Bridge/",
			"Tests\\MeiliSearchBundle\\Bridge\\Doctrine\\": "tests/Bridge/Doctrine/",
			"Tests\\MeiliSearchBundle\\Bridge\\Doctrine\\Attribute\\": "tests/Bridge/Doctrine/Attribute/",
			"Tests\\MeiliSearchBundle\\Bridge\\Doctrine\\Attribute\\Reader\\": "tests/Bridge/Doctrine/Attribute/Reader/",
			"Tests\\MeiliSearchBundle\\Bridge\\Doctrine\\EventSubscriber\\": "tests/Bridge/Doctrine/EventSubscriber/",
			"Tests\\MeiliSearchBundle\\Bridge\\Doctrine\\Serializer\\": "tests/Bridge/Doctrine/Serializer/",
			"Tests\\MeiliSearchBundle\\Cache\\": "tests/Cache/",
			"Tests\\MeiliSearchBundle\\Command\\": "tests/Command/",
			"Tests\\MeiliSearchBundle\\DataCollector\\": "tests/DataCollector/",
			"Tests\\MeiliSearchBundle\\DependencyInjection\\": "tests/DependencyInjection/",
			"Tests\\MeiliSearchBundle\\Document\\": "tests/Document/",
			"Tests\\MeiliSearchBundle\\Dump\\": "tests/Dump/",
			"Tests\\MeiliSearchBundle\\Event\\": "tests/Event/",
			"Tests\\MeiliSearchBundle\\Event\\Document\\": "tests/Event/Document/",
			"Tests\\MeiliSearchBundle\\Event\\Dump\\": "tests/Event/Dump/",
			"Tests\\MeiliSearchBundle\\Event\\Index\\": "tests/Event/Index/",
			"Tests\\MeiliSearchBundle\\EventSubscriber\\": "tests/EventSubscriber/",
			"Tests\\MeiliSearchBundle\\Form\\": "tests/Form/",
			"Tests\\MeiliSearchBundle\\Form\\Type\\": "tests/Form/Type/",
			"Tests\\MeiliSearchBundle\\Health\\": "tests/Health/",
			"Tests\\MeiliSearchBundle\\Index\\": "tests/Index/",
			"Tests\\MeiliSearchBundle\\Messenger\\": "tests/Messenger/",
			"Tests\\MeiliSearchBundle\\Messenger\\Document\\": "tests/Messenger/Document/",
			"Tests\\MeiliSearchBundle\\Messenger\\Handler\\": "tests/Messenger/Handler/",
			"Tests\\MeiliSearchBundle\\Messenger\\Handler\\Document\\": "tests/Messenger/Handler/Document/",
			"Tests\\MeiliSearchBundle\\Messenger\\Handler\\Synonyms\\": "tests/Messenger/Handler/Synonyms/",
			"Tests\\MeiliSearchBundle\\Messenger\\Synonyms\\": "tests/Messenger/Synonyms/",
			"Tests\\MeiliSearchBundle\\Metadata\\": "tests/Metadata/",
			"Tests\\MeiliSearchBundle\\Result\\": "tests/Result/",
			"Tests\\MeiliSearchBundle\\Search\\": "tests/Search/",
			"Tests\\MeiliSearchBundle\\Twig\\": "tests/Twig/",
			"Tests\\MeiliSearchBundle\\Update\\": "tests/Update/"
		}
	},
	"config": {
		"classmap-authoritative": true,
		"optimize-autoloader": true,
		"allow-plugins": {
			"infection/extension-installer": true,
			"php-http/discovery": true,
			"phpstan/extension-installer": true
		}
	},
	"require": {
		"php": ">=8.1",
		"ext-json": "*",
		"meilisearch/meilisearch-php": "^1.4",
		"psr/log": "^1.1|^2|^3",
		"symfony/config": "^6.1|^7.0",
		"symfony/console": "^6.1|^7.0",
		"symfony/dependency-injection": "^6.1|^7.0",
		"symfony/filesystem": "^6.1|^7.0",
		"symfony/finder": "^6.1|^7.0",
		"symfony/http-kernel": "^6.1|^7.0",
		"symfony/property-access": "^6.1|^7.0",
		"symfony/options-resolver": "^6.1|^7.0",
		"symfony/serializer": "^6.1|^7.0"
	},
	"require-dev": {
		"api-platform/core": "^2.5",
		"doctrine/orm": "^2.16",
		"friendsofphp/php-cs-fixer": "^3.38",
		"infection/infection": "^0.27.8",
		"phpstan/phpstan": "^1.10",
		"phpunit/phpunit": "^9.6",
		"ramsey/uuid": "^4.7",
		"rector/rector": "^0.18",
		"symfony/cache": "^6.1|^7.0",
		"symfony/event-dispatcher": "^6.1|^7.0",
		"symfony/expression-language": "^6.1|^7.0",
		"symfony/form": "^6.1|^7.0",
		"symfony/http-client": "^6.1|^7.0",
		"symfony/messenger": "^6.1|^7.0",
		"twig/twig": ">=2.7",
		"phpstan/phpstan-phpunit": "^1.3",
		"phpstan/extension-installer": "^1.3"
	},
	"suggest": {
		"doctrine/orm": "Allow to listen to entity events and trigger related MeiliSearch actions",
		"symfony/cache": "Allow to use the CachedSearchEntryPoint to improve performances",
		"symfony/form": "Allow to use custom types which brings search features",
		"symfony/messenger": "Allow to dispatch messages and delay some actions (like index|documents creation)",
		"twig/twig": "Allow to use the search features in Twig templates"
	},
	"extra": {
		"phpstan": {
			"includes": [
				"extension.neon"
			]
		}
	}
}
